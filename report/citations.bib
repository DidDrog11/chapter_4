	file = {Full Text:C\:\\Users\\ucbtds4\\Zotero\\storage\\WDMF384T\\Kline et al. - 2021 - Clinical prediction rule for SARS-CoV-2 infection .pdf:application/pdf;Full Text:C\:\\Users\\ucbtds4\\Zotero\\storage\\JWA7D34S\\Kline et al. - 2021 - Clinical prediction rule for SARS-CoV-2 infection .pdf:application/pdf},
